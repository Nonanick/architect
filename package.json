{
  "name": "svelte-app",
  "version": "1.0.0",
  "main": "./src/app/app.boot.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "node ./bin/run.js",
    "dev:ui": "rollup -c -w",
    "dev:server": "nodemon --watch src/server --watch src/lib --exec node src/server/server.fastify.js",
    "dev:build-app": "tsc -p ./tsconfig.electron.json -w",
    "dev:app": "nodemon --watch src/app --watch src/lib --exec electron ./src/app/app.boot.js",
    "start": "sirv public",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/node": "^14.14.32",
    "chokidar": "^3.5.1",
    "electron": "^18.3.7",
    "fastify-cors": "^5.2.0",
    "nodemon": "^2.0.7",
    "rollup": "^2.40.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^1.0.11",
    "svelte": "^3.35.0",
    "svelte-check": "^1.2.3",
    "svelte-preprocess": "^4.6.9",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@editorjs/checklist": "^1.3.0",
    "@editorjs/editorjs": "^2.19.1",
    "@editorjs/header": "^2.6.1",
    "@editorjs/list": "^1.6.2",
    "@editorjs/marker": "^1.2.2",
    "@editorjs/table": "^1.3.0",
    "dotenv": "^8.2.0",
    "electron-store": "^7.0.2",
    "esm": "^3.2.25",
    "fast-glob": "^3.2.5",
    "highlight.js": "^10.6.0",
    "nanoid": "^3.1.20",
    "path-to-regexp": "^6.2.0",
    "v8-compile-cache": "^2.3.0"
  },
  "license": "GPL-3.0"
}
