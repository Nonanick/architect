{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev:ui": "rollup -c -w",
    "dev:build": "tsc -p ./tsconfig.electron.json -w",
    "dev:app": "nodemon --watch src/app --watch src/server --exec electron ./src/app/app.boot.js",
    "start": "sirv public",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "@tsconfig/svelte": "^1.0.0",
    "electron": "^11.2.1",
    "nodemon": "^2.0.7",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sirv-cli": "^1.0.10",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "electron-store": "^7.0.0",
    "nanoid": "^3.1.20",
    "path-to-regexp": "^6.2.0",
    "v8-compile-cache": "^2.2.0"
  },
  "license": "GPL-3.0"
}
