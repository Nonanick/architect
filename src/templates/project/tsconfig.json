{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "lib": [
      "ESNext"
    ],
    "allowJs": true,
    "importHelpers": true,
    "rootDir": "./",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "@architect/*": [
        "./.architect/typings/*"
      ],
      "@dtos/*": [
        "./src/data/dtos/*"
      ]
    }
  },
  "include": [
    "src",".architect/typings"
  ]
}