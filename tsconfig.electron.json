{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES6",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "importHelpers": true,
    "strict": true,
    "sourceMap": false,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "baseUrl": "src",
    "experimentalDecorators": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "dev",
    "src/ui",
    "src/templates",
    "node_modules",
    "public",
    ".vscode"
  ]
}